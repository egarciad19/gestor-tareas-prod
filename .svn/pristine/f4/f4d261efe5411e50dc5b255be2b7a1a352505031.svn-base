<h2 mat-dialog-title>Crear Tarea</h2>
<mat-dialog-content>
    <div class="formulario">
        <h3>Lista: {{data.nombre}}</h3>
        <mat-card-content>
            <form class="example-form">
                <mat-form-field class="example-full-width" appearance="outline">
                    <mat-label>Nombre Tarea</mat-label>
                    <input matInput autocomplete="off" [formControl]="nombreTarea">
                    <mat-error>Este es un campo obligatorio</mat-error>
                </mat-form-field>

                <mat-form-field class="example-full-width" appearance="outline">
                    <mat-label>Descripci√≥n</mat-label>
                    <textarea matInput autocomplete="off" rows="3" cdkTextareaAutosize="true"
                        [formControl]="descripcionTarea"></textarea>
                </mat-form-field>


            </form>
        </mat-card-content>
    </div>
</mat-dialog-content>
<mat-dialog-actions>
    <button  mat-raised-button color="warn" [mat-dialog-close]>Cancelar</button>
    <button mat-raised-button color="primary" (click)="closeDialog()">Crear Tarea</button>
</mat-dialog-actions>